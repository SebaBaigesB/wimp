<div class="container">
  <%= simple_form_for(@animal) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :food_style %>
    <%= f.input :front_photo %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit class: "btn btn-primary"%>
  <% end %>
</div>
